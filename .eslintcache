Router>\n        <Switch>\n          <Route\n            exact\n            path=\"/\"\n            render={() => (\n              <LoginPage\n                apiClient={apiClient}\n                loggedIn={loggedIn}\n                logInFunc={loginFunction}\n              />\n            )}\n          />\n          <ProtectedRoute exact path=\"/events\" loggedIn={loggedIn}>\n            <EventsPage loggedIn={loggedIn} apiClient={apiClient} user={user} />\n          </ProtectedRoute>\n          <ProtectedRoute exact path=\"/events/add\" loggedIn={loggedIn}>\n            <AddEvent loggedIn={loggedIn} apiClient={apiClient} user={user} />\n          </ProtectedRoute>\n          <ProtectedRoute exact path=\"/events/update/:id\" loggedIn={loggedIn}>\n            <UpdateEvent\n              loggedIn={loggedIn}\n              events={allEvents}\n              apiClient={apiClient}\n              user={user}\n            />\n          </ProtectedRoute>\n          <ProtectedRoute exact path=\"/profile\" loggedIn={loggedIn}>\n            <AccountPage\n              loggedIn={loggedIn}\n              logout={logoutFunction}\n              events={allEvents}\n              apiClient={apiClient}\n              user={user}\n            />\n          </ProtectedRoute>\n        </Switch>\n      </Router>\n    </div>\n  );\n};\nexport default App;\n","C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\reportWebVitals.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\apiClient.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\components\\protectedRoute.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\pages\\index.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\pages\\LoginPage.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full-stack-events-app\\src\\pages\\AddEvent.js",[],"C:\\Users\\micha\\Javascript-course\\mongoDB\\full